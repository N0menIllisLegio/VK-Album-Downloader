<Window
    x:Class="AlbumDownloader.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:local="clr-namespace:AlbumDownloader"
    Title="{Binding Title}"
    Width="525"
    Height="350"
    Icon="pack://application:,,,/AlbumDownloader;component/image.ico"
    prism:ViewModelLocator.AutoWireViewModel="True">
  <ContentControl prism:RegionManager.RegionName="{x:Static local:Settings.MainWindowRegion}" />
</Window>
